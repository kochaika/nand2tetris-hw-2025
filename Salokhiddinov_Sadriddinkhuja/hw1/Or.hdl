/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = notout1);
    Not(in = b, out = notout2);
    Nand(a = notout1, b = notout2, out = out);
}