/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */


  CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:

    Nand(a=sel,b=sel,out=selnot);
    Nand(a=selnot,b=in,out=A);
    Nand(a=A,b=A,out=a);
    Nand(a=sel,b=in,out=B);
    Nand(a=B,b=B,out=b);


}




