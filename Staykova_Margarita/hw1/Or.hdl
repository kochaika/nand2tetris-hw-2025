/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=outNot1);
    Not(in=b, out=outNot2);
    Nand(a=outNot1, b=outNot2, out=out);
}