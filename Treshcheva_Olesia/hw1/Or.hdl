/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a= a, b= a, out= o1);
    Nand(a= b, b= b, out= o2);
    And(a= o1, b= o2, out= o3);
    Nand(a= o3, b= o3, out= out);
}