/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=a, b=sel, out=out1);
    Not(in=sel, out=sel2);
    And(a=b, b=sel2, out=out2);
    Or(a=out1, b=out2, out=out);
    
    
}