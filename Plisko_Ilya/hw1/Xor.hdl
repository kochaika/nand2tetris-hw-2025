/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nab);
    Nand(a=a, b=nab, out=a1);
    Nand(a=nab, b=b, out=b1);
    Nand(a=a1, b=b1, out=out);
}