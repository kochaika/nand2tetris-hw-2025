/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    //notSel
    Nand(a=sel, b=sel, out=notSel);

    //a=in AND notSel
    Nand(a=in, b=notSel, out=nand1);
    Nand(a=nand1, b=nand1, out=a);  

    //b=in AND sel
    Nand(a=in, b=sel, out=nand2);
    Nand(a=nand2, b=nand2, out=b);  
}